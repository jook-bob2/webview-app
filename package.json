{
  "name": "eblabsapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "env:loc": "node scripts/set-environment.js local",
    "env:dev": "node scripts/set-environment.js development",
    "env:prod": "node scripts/set-environment.js production",
    "_ios": "react-native run-ios",
    "_android": "react-native run-android",
    "ios:loc": "yarn env:loc && yarn _ios",
    "ios:dev": "yarn env:dev && yarn _ios",
    "ios:prod": "yarn env:prod && yarn _ios",
    "android:loc": "yarn env:loc && yarn _android && yarn android:adb",
    "android:dev": "yarn env:dev && yarn _android",
    "android:prod": "yarn env:prod && yarn _android",
    "_build:ios": "react-native bundle --dev false --entry-file index.jsx --bundle-output ios/main.jsbundle --assets-dest ./ios --platform ios",
    "build:ios:loc": "yarn env:loc && yarn _build:ios",
    "build:ios:dev": "yarn env:dev && yarn _build:ios",
    "build:ios:prod": "yarn env:prod && yarn _build:ios",
    "_build:android": "react-native bundle --platform android --dev false --entry-file index.jsx --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/react",
    "build:android:loc": "yarn env:loc && yarn _build:android",
    "build:android:dev": "yarn env:dev && yarn _build:android",
    "build:android:prod": "yarn env:prod && yarn _build:android",
    "android:adb": "adb -s emulator-5554 reverse tcp:8080 tcp:8080",
    "test": "jest",
    "lint": "eslint --ext .jsx --ext .js src/",
    "lint:fix": "eslint --ext .jsx --ext .js src/ --fix",
    "fast:build": "cd ios && fastlane firebase type:build",
    "fast:version": "cd ios && fastlane firebase type:version",
    "firebase:dev:build": "yarn build:ios:dev && yarn fast:build",
    "firebase:dev:version": "yarn build:ios:dev && yarn fast:version"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.68.1",
    "react-native-webview": "^11.18.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^28.1.3",
    "@types/react": "^18.0.14",
    "@types/react-native": "^0.69.1",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.7.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
